; delsub.ins Notepad++
; Borrmann, 2014-07-31

set $DisplayName$ = GetRegistryStringValue32("[HKLM\SOFTWARE\Wow6432Node\Microsoft\Windows\CurrentVersion\Uninstall\Notepad++] UninstallString")
if not ($DisplayName$ = "")
	Message "Uninstalling previous Installation"
	Winbatch_Uninstall
	Files_Delete_InstallDir
endif

[Winbatch_Uninstall]
$DisplayName$ /S

[Files_Delete_InstallDir]
delete -sf "$InstallDir$\"